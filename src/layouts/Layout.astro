---
export interface Props {
	title: string;
}

const { title } = Astro.props;

---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<meta name="description" content="Petaluma California, Luma Skatepark Coalition, A grassroots movement to revitalize and build new skateparks utilizing Measure M monies in Petaluma. Petaluma, the 2nd largest city in Sonoma County California, deserves new and updated skateparks to support the community's love for skateboarding, Petaluma Skatepark ">
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<link rel="stylesheet" href="../css/style.css">

	</head>
	<body>
		<slot />
	</body>
</html>


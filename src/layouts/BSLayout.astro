---
//import radSkater from '../images/sm-radSkater.png';
export interface Props {
	title: string;
}

const { title } = Astro.props;

---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<meta name="description" content="Petaluma, Luma Skatepark Coalition, Somoma County Skateparks, Building Good Skateparks, Petaluma Skatepark ">
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">

		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<link rel="stylesheet" href="../css/style.css">

	</head>
	<body>
        <header>
          <nav class="navbar  navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="#"><img src='' alt='Luma Skatepark Coalition' /></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
              <ul class="navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link" href="../index.html">Home</a>
                </li>
                <!--
                  <li class="nav-item">
                  <a class="nav-link" href="/donate/">Donate</a>
              </li>
                 -->
                <li class="nav-item">
                  <a class="nav-link" href="/blog/">Blog</a>
                </li>
              </ul>
            </div>
          </nav>
        </header>
		<slot />

	</body>
</html>

<style>
  .navbar-nav{
    margin-left:1em;
  }
</style>

